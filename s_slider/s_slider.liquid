<div id="sslider__wrap--{{section.id}}">
  <div id="sslider__content--{{section.id}}">
    <section class="cd-hero js-cd-hero {% if section.settings.autoplay %}js-cd-autoplay{% endif %}">
      <ul class="cd-hero__slider">
        {% for block in section.blocks %}
        {% if block.type == 'image' %}
        <li class="cd-hero__slide js-cd-slide {% if forloop.first %}cd-hero__slide--selected{% endif %}" 
            style="background-color:{{block.settings.background}};{% if block.settings.background-img != blank %}background-image:url({{block.settings.background-img | img_url:'master'}}){% endif %}" {{ block.shopify_attributes }}>
          {% if block.settings.hero-image != blank %}
          {% capture image %}
          <div class="cd-hero__content cd-hero__content--half-width cd-hero__content--img">
            <img src="{{block.settings.hero-image | img_url:'900x'}}" alt="{{block.settings.title}}">
          </div>
          {% endcapture %}
          {% else %}
          {% capture image %}{% endcapture %}
          {% endif %}

          {% if block.settings.hero-position == 'left' %}{{image}}{% endif %}

          <div class="cd-hero__content cd-hero__content--{{block.id}} {% if block.settings.hero-image != blank %}cd-hero__content--half-width{% else %}cd-hero__content--full-width{% endif %} ">
            <div class="cd-hero__inner">
              {% if block.settings.title != blank %}
              <h2 class="js-s-fz" data-fz="{{block.settings.title_fz}}" style="color:{{block.settings.title_cl}};font-size:{{block.settings.title_fz}}px">{{block.settings.title}}</h2>
              {% endif %}
              {% if block.settings.subtitle != blank %}
              <p class="js-s-fz" data-fz="{{block.settings.subtitle_fz}}" style="color:{{block.settings.subtitle_cl}};font-size:{{block.settings.subtitle_fz}}px">{{block.settings.subtitle}}</p>
              {% endif %}
              {% if block.settings.button-link-1 != blank and block.settings.button-text-1 != blank %}
              <a href="{{block.settings.button-link-1}}" title="{{block.settings.button-text-1}}" class="cd-hero__btn" style="background-color:{{block.settings.button-bg-1}}">
                {{block.settings.button-text-1}}
              </a>
              {% endif %}
              {% if block.settings.button-link-2 != blank and block.settings.button-text-2 != blank %}
              <a href="{{block.settings.button-link-2}}" title="{{block.settings.button-text-2}}" class="cd-hero__btn cd-hero__btn--secondary" style="background-color:{{block.settings.button-bg-2}}">
                {{block.settings.button-text-2}}
              </a>
              {% endif %}
            </div>
          </div>
          {% if block.settings.hero-position == 'right' %}{{image}}{% endif %}
        </li>
        {% endif %}

        {% if block.type == 'video' %}

        <li class="cd-hero__slide cd-hero__slide--video js-cd-slide {% if forloop.first %}cd-hero__slide--selected{% endif %}" {{ block.shopify_attributes }}>
          <div class="cd-hero__content cd-hero__content--{{block.id}} cd-hero__content--full-width">
            <div class="cd-hero__inner">
              {% if block.settings.title != blank %}
              <h2 class="js-s-fz" data-fz="{{block.settings.title_fz}}" style="color:{{block.settings.title_cl}};font-size:{{block.settings.title_fz}}px">{{block.settings.title}}</h2>
              {% endif %}
              {% if block.settings.subtitle != blank %}
              <p class="js-s-fz" data-fz="{{block.settings.subtitle_fz}}" style="color:{{block.settings.subtitle_cl}};font-size:{{block.settings.subtitle_fz}}px">{{block.settings.subtitle}}</p>
              {% endif %}
              {% if block.settings.button-link-1 != blank and block.settings.button-text-1 != blank %}
              <a href="{{block.settings.button-link-1}}" title="{{block.settings.button-text-1}}" class="cd-hero__btn" style="background-color:{{block.settings.button-bg-1}}">
                {{block.settings.button-text-1}}
              </a>
              {% endif %}
            </div>
            <div class="cd-hero__content cd-hero__content--bg-video js-cd-bg-video" data-video="{{block.settings.video}}"></div>
          </div>
        </li>

        {% endif %}
        {% endfor %}
      </ul>

      <div class="cd-hero__nav js-cd-nav">
        <nav>
          <span class="cd-hero__marker cd-hero__marker--item-1 js-cd-marker"></span>
          <ul>
            {% for block in section.blocks %}
            <li {% if forloop.first %}class="cd-selected"{% endif %}>
              <a href="javascript:void(0)">
                {{block.settings.icon}}
                {{block.settings.nav}}
              </a>
            </li>
            {% endfor %}
          </ul>
        </nav> 
      </div>
    </section>
  </div>
</div>

